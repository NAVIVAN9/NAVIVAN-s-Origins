{
	"name": "Dimension Hop",
	"description": "You can teleport to any dimension by holding an item that represents it and then pressing the primary power button. If you aren't holding a representative item, then you will instead open an inventory made with the intention of holding the aforementioned items.",

	"type": "origins:multiple",

	"travel_to_end": {
		"type": "origins:active_self",
		"cooldown": 10,
		"hud_render": {
			"should_render": false
		},
		"entity_action": {
			"type": "origins:and",
			"actions": [{
					"type": "origins:execute_command",
					"command": "particle minecraft:instant_effect ~ ~0.5 ~ 0.5 0.5 0.5 0.1 16 normal",
					"permission_level": 2
				},
				{
					"type": "origins:delay",
					"ticks": 1,
					"action": {
						"type": "origins:execute_command",
						"command": "execute in minecraft:the_end run tp ~ ~ ~",
						"permission_level": 2
					}
				}
			]
		},
		"condition": {
			"type": "origins:and",
			"conditions": [{
