{
	"type": "origins:multiple",
	"trigger": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:and",
			"actions": [{
				"type": "origins:trigger_cooldown",
				"power": "infovore:invisent_cooldown"
			}]
		},
		"cooldown": 200,
		"hud_render": {
			"should_render": true,
			"sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
			"bar_index": 7
		}
	},
	"cooldown": {
		"type": "origins:cooldown",
		"cooldown": 100,
		"hud_render": {
			"should_render": false
		}
	},
	"render": {
		"type": "origins:prevent_entity_render",
		"entity_condition": {
			"type": "origins:entity_type",
			"entity_type": "minecraft:player",
			"inverted": true
		},
		"bientity_condition": {
			"type": "origins:relative_rotation",
			"actor_rotation": "head",
			"target_rotation": "body",
			"comparison": ">=",
			"compare_to": -0.4
		},
		"condition": {
			"type": "origins:and",
			"conditions": [{
				"type": "origins:resource",
				"resource": "infovore:invisent_cooldown",
				"comparison": "<=",
				"compare_to": -0.2
			}]
		}
	},
	"glow": {
		"type": "origins:entity_glow",
		"bientity_condition": {
			"type": "origins:can_see"
		}
	},
	"name": "invisble entities",
	"description": "all entities are invisible. If you press the primary power button they will be visible for 5 seconds, cooldown of 20 seconds."
}